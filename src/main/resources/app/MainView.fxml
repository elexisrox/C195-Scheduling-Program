<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="250.0" prefWidth="250.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controller.MainViewController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Button fx:id="showcustomers" onAction="#onCustButtonClick" text="Show Countries" />

    <Label fx:id="welcomeText" />
   <TableView fx:id="exampleTable" prefHeight="200.0" prefWidth="200.0">
     <columns>
       <TableColumn fx:id="testcolumn1" prefWidth="105.0" text="Country ID" />
       <TableColumn fx:id="testcolumn2" prefWidth="105.0" text="Country Name" />
     </columns>
   </TableView>
</VBox>
